  body
    .navbar.navbar-fixed-top.navbar-inverse
      .navbar-inner
        .container
          // Menu button for smallar screens
          a.btn.btn-navbar(data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          // Site name for smallar screens
          a.brand(href='index-2.html')
            | Site
            span.bold Name
          // Navigation starts
          .nav-collapse.collapse
            // Links
            ul.nav.pull-right
              li.dropdown.pull-right
                a.dropdown-toggle(data-toggle='dropdown', href='#')
                  img.nav-user-pic(src='img/user.jpg', alt='')
                  | Admin
                  b.caret
                // Dropdown menu
                ul.dropdown-menu
                  li
                    a(href='#')
                      i.icon-user
                      | Profile
                  li
                    a(href='#')
                      i.icon-cogs
                      | Settings
                  li#btn-logout
                    a(href='#')
                      i.icon-off
                      | Logout
    // Main content starts
    .content
      // Sidebar
      include sidebar
      // Main bar
      .mainbar
        // Page heading
        .page-head
          // Page heading
          h2.pull-left
            | Dashboard
            // page meta
            span.page-meta Something Goes Here
          // Breadcrumb
          .bread-crumb.pull-right
            a(href='index-2.html')
              i.icon-home
              | Home
            // Divider
            span.divider /
            a.bread-current(href='#') Dashboard
          .clearfix
        // Page heading ends
        // Matter
        .matter
          .container-fluid
            | HELLO
          // Matter ends
          // modals
          .modal-form-errors.modal.hide.fade
            .modal-header
              button.close(data-dismiss='modal') x
              h3 Whoops!
            .modal-body
              p.subheading It looks like there was a problem.
              ul
            .modal-footer
              button.btn.btn-warning(data-dismiss='modal') OK
          .modal-alert.modal.hide.fade
            .modal-header
              button.close(data-dismiss='modal') x
              h3
            .modal-body
              p
            .modal-footer
              button#ok.btn.btn-warning(data-dismiss='modal') OK
          .modal-confirm.modal.hide.fade
            .modal-header
              button.close(data-dismiss='modal') x
              h3 Delete Account
            .modal-body
              p Are you sure you want to delete your account?
            .modal-footer
              button.cancel.btn(data-dismiss='modal') Cancel
              button.submit.btn.btn-danger Delete
          // end of modals
        // Mainbar ends
        .clearfix
      // Content ends
      // Scroll to top
      span.totop
        a(href='#')
          i.icon-chevron-up
      // modals
